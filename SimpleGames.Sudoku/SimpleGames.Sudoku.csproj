<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Dmitry Bychenko</Authors>
    <Product>SimpleGames Sudoku</Product>
    <Description>Sudoku Solver and Analyser</Description>
    <Copyright>Copyright (c) 2021, Dmitry Bychenko</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Dmitry-Bychenko/SimpleGames.Sudoku</RepositoryUrl>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>SimpleGames.Sudoku.snk.pfx</AssemblyOriginatorKeyFile>
    <PackageIcon>Sudoku.png</PackageIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0.1</Version>
    <PackageReleaseNotes>Better solver (Kernel based)</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\Images\Sudoku.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
